{
  "@select": "*",
  "@where": {
    "@graph": {"@id": "?a", "?b": "?c"},
    "@filter": [
      {
        "@or": [
          {"@eq": ["?a", 1]},
          {"@eq": ["?a", 2]},
          {"@eq": ["?a", 3]},
          {"@eq": ["?a", 4]}
        ]
      },
      {
        "@and": [
          {"@eq": ["?a", 1]},
          {"@eq": ["?a", 2]},
          {"@eq": ["?a", 3]},
          {"@eq": ["?a", 4]}
        ]
      },
      {
        "@or": [
          {"@and": [{"@eq": ["?a", 1]}, {"@eq": ["?a", 2]}]},
          {"@and": [{"@eq": ["?a", 3]}, {"@eq": ["?a", 4]}]}
        ]
      }
    ]
  }
}